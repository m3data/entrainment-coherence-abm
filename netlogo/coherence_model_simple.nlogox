<model version="NetLogo 7.0.3" snapToGrid="true">
  <code><![CDATA[;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; COHERENCE vs ENTRAINMENT MODEL
;; Illustrative agent-based dynamical sketch
;; Recovery Tracking v2 + Fatigue Extension v1
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

globals [
  perturbing?              ;; whether a perturbation is active
  perturb-timer
  max-variance
  perturb-started?
  last-perturb-strength
  perturb-schedule

  ;; Recovery tracking v2
  stable-baseline          ;; rolling average before perturbation
  baseline-window          ;; list for rolling calculation
  perturb-end-tick         ;; when perturbation finished
  peak-deviation           ;; max deviation from baseline during/after perturbation
  recovery-complete?       ;; flag for clean recovery detection
  last-recovery-time       ;; ticks from perturbation end to tolerance return

  ;; Initial conditions tracking (I001)
  initial-heading-variance ;; heading variance at tick 0 (before any dynamics)

  ;; Agent parameter heterogeneity tracking (I002)
  initial-coupling-bias-mean
  initial-coupling-bias-variance
  initial-inertia-mean
  initial-inertia-variance

  ;; Pre-perturbation state tracking (I002)
  pre-perturbation-cost      ;; mean-cumulative-cost at tick 299
  pre-perturbation-variance  ;; heading-variance at tick 299
  pre-perturbation-captured? ;; flag to capture only once
]

turtles-own [
  preferred-heading    ;; internal tendency (identity proxy)
  coupling-bias        ;; individual sensitivity to alignment (0-1)
  inertia              ;; heading update inertia (0–1)
  tie-strength         ;; how influential this turtle is to others (0–1)

  ;; Cost tracking (v1)
  metabolic-cost       ;; accumulated regulatory cost
  shock-cost           ;; accumulated perturbation cost
  recovery-cost        ;; accumulated recovery cost
  last-heading         ;; for computing heading-change

  ;; Configuration tracking (I004)
  initial-distance-from-mean  ;; angular distance from group mean at setup
  alignment-work              ;; cumulative angular displacement toward social field

  ;; C001: Selective escape valve
  is-load-bearer?             ;; true if coupling-bias > 0.7 AND initial-distance > 45°
]

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; SETUP
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

to setup
  clear-all
  set max-variance 180
  set perturbing? false
  set perturb-timer 0
  set perturb-started? false
  set last-perturb-strength 0

  ;; Recovery tracking v2 init
  set baseline-window []
  set stable-baseline 0
  set perturb-end-tick 0
  set peak-deviation 0
  set recovery-complete? true
  set last-recovery-time -1  ;; -1 means no recovery measured yet

  create-turtles population [
    setxy random-xcor random-ycor
    set heading random 360
    set preferred-heading heading

    ;; Coupling-bias initialization (I003: controlled variance support)
    ifelse controlled-variance? [
      ;; For uniform on [a,b], variance = (b-a)²/12
      ;; For target variance v: half-range = sqrt(3*v)
      let half-range sqrt (3 * target-coupling-bias-variance)
      let raw-value (0.5 - half-range) + random-float (2 * half-range)
      ;; Clip to [0.01, 0.99] to avoid edge effects
      set coupling-bias max list 0.01 (min list 0.99 raw-value)
    ] [
      set coupling-bias random-float 1
    ]

    set inertia (0.2 + random-float 0.8)
    set tie-strength (0.2 + random-float 0.8)
    set shape "agent-field"
    set color scale-color blue coupling-bias 0 1
    set size 1 + (2 * tie-strength)

    ;; Initialize cost tracking
    set metabolic-cost 0
    set shock-cost 0
    set recovery-cost 0
    set last-heading heading

    ;; Initialize I004 tracking
    set alignment-work 0
    set initial-distance-from-mean 0  ;; computed after all agents created
  ]

  ;; Compute initial-distance-from-mean and classify load-bearers (I004, C001)
  ;; Must happen after all agents exist so we can calculate group mean
  let mean-heading mean-heading-of-turtles
  ask turtles [
    set initial-distance-from-mean abs subtract-headings heading mean-heading
    set is-load-bearer? (coupling-bias > 0.7 and initial-distance-from-mean > 45)
  ]

  ;; Perturbation schedule
  set perturb-schedule []

  if perturbation-regime = "single" [
    set perturb-schedule (list 300)
  ]

  if perturbation-regime = "periodic" [
    set perturb-schedule n-values 10 [ i -> 300 + (i * 300) ]
  ]

  if perturbation-regime = "irregular" [
    set perturb-schedule sort n-of 5 (range 200 2000)
  ]

  ;; --- plot initialisation ---
  set-current-plot "Heading Variance Over Time"
  clear-plot

  ;; Capture initial conditions (I001)
  set initial-heading-variance heading-variance

  ;; Capture agent parameter heterogeneity (I002)
  set initial-coupling-bias-mean mean [coupling-bias] of turtles
  set initial-coupling-bias-variance variance [coupling-bias] of turtles
  set initial-inertia-mean mean [inertia] of turtles
  set initial-inertia-variance variance [inertia] of turtles

  ;; Initialize pre-perturbation tracking (I002)
  set pre-perturbation-cost 0
  set pre-perturbation-variance 0
  set pre-perturbation-captured? false

  reset-ticks
end

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; MAIN LOOP
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

to go
  ;; --- Capture pre-perturbation state (I002) ---
  ;; Capture at tick 299, just before first perturbation at tick 300
  if ticks = 299 and not pre-perturbation-captured? [
    set pre-perturbation-cost mean-cumulative-cost
    set pre-perturbation-variance heading-variance
    set pre-perturbation-captured? true
  ]

  ;; --- Update rolling baseline (only when stable: not perturbing and recovered) ---
  if (not perturbing?) and recovery-complete? [
    set baseline-window lput heading-variance baseline-window
    ;; Keep window at 50 ticks max
    if length baseline-window > 50 [
      set baseline-window but-first baseline-window
    ]
    ;; Only compute baseline once we have enough samples
    if length baseline-window >= 10 [
      set stable-baseline mean baseline-window
    ]
  ]

  ;; --- Apply ongoing perturbation ---
  if perturbing? [
    apply-perturbation
  ]

  ;; --- Scheduled perturbations ---
  if member? ticks perturb-schedule [
    perturb
  ]

  ;; --- Agent dynamics ---
  ask turtles [
    update-heading
    apply-noise
    move
  ]

  ;; --- Cost accumulation ---
  ask turtles [
    let heading-change abs subtract-headings heading last-heading

    ;; Metabolic cost: always accruing (continuous regulatory effort)
    set metabolic-cost metabolic-cost + (heading-change * metabolic-rate)

    ;; Shock cost: only during active perturbation
    if perturbing? [
      set shock-cost shock-cost + (heading-change * shock-multiplier)
    ]

    ;; Recovery cost: during recovery period (after perturbation ends)
    ;; Mechanism D: Fatigue slows recovery (increases recovery cost accumulation)
    if (not recovery-complete?) and (not perturbing?) [
      let recovery-rate-adjusted recovery-rate
      if fatigue-enabled? [
        let my-fatigue-level calculate-fatigue-level
        ;; At max fatigue, recovery costs accumulate up to 2× faster
        set recovery-rate-adjusted recovery-rate * (1 + fatigue-intensity * my-fatigue-level)
      ]
      set recovery-cost recovery-cost + (heading-change * recovery-rate-adjusted)
    ]

    ;; Update last-heading for next tick
    set last-heading heading
  ]

  ;; --- Cost decay during stable periods ---
  ;; Slow decay: costs decrease when system is stable (recovered and not perturbing)
  if fatigue-enabled? and recovery-complete? and not perturbing? [
    ask turtles [
      set metabolic-cost metabolic-cost * (1 - cost-decay-rate)
      set shock-cost shock-cost * (1 - cost-decay-rate)
      set recovery-cost recovery-cost * (1 - cost-decay-rate)
    ]
  ]

  ;; --- Track peak deviation during and after perturbation ---
  if (not recovery-complete?) [
    let current-deviation abs (heading-variance - stable-baseline)
    if current-deviation > peak-deviation [
      set peak-deviation current-deviation
    ]
  ]

  ;; --- Recovery tracking v2 ---
  ;; Recovery is measured from perturbation END to when variance returns within tolerance of stable baseline
  if (not recovery-complete?) and (not perturbing?) [
    let current-deviation abs (heading-variance - stable-baseline)

    ;; Recovery complete when within tolerance band of stable baseline
    if current-deviation <= recovery-tolerance [
      set last-recovery-time (ticks - perturb-end-tick)
      set recovery-complete? true
    ]
  ]

  ;; --- Plotting ---
  set-current-plot "Heading Variance Over Time"
  set-current-plot-pen "variance"
  plot heading-variance

  set-current-plot-pen "perturb"
  if perturb-started? [
    plot-pen-reset
    plotxy ticks 0
    plotxy ticks ((last-perturb-strength / 360) * max-variance)
    set perturb-started? false
  ]

  tick
end

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; DYNAMICS
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

to update-heading
  let nearby-turtles turtles in-radius 3

  if any? nearby-turtles [
    let total-weight sum [tie-strength] of nearby-turtles

    let weighted-x sum [ tie-strength * sin heading ] of nearby-turtles
    let weighted-y sum [ tie-strength * cos heading ] of nearby-turtles
    let avg-heading atan weighted-x weighted-y

    let desired-turn 0
    let social-component 0  ;; I004: track social alignment work separately

    if entrainment-mode? [
      set social-component (coupling-strength * coupling-bias * subtract-headings avg-heading heading)
      set desired-turn social-component
    ]

    if not entrainment-mode? [
      let identity-pull subtract-headings preferred-heading heading
      let social-pull subtract-headings avg-heading heading

      ;; C001: Selective identity-pull based on agent classification
      let apply-identity-pull? (
        selective-identity-pull = "all" or
        (selective-identity-pull = "load-bearers-only" and is-load-bearer?) or
        (selective-identity-pull = "non-load-bearers-only" and not is-load-bearer?)
      )

      set social-component (coupling-strength * coupling-bias * social-pull-weight * social-pull)
      ifelse apply-identity-pull? [
        set desired-turn social-component + (identity-pull-weight * identity-pull)
      ] [
        set desired-turn social-component
      ]
    ]

    ;; Mechanism B: Fatigue increases effective inertia (reduces turning capacity)
    let effective-inertia inertia
    if fatigue-enabled? [
      let my-fatigue-level calculate-fatigue-level
      ;; At max fatigue (level=1, intensity=1), effective-inertia → 1.0 (no turning)
      set effective-inertia inertia + ((1 - inertia) * fatigue-intensity * my-fatigue-level)
    ]

    ;; I004: Track alignment work (social pull component actually executed)
    let actual-social-work abs ((1 - effective-inertia) * social-component)
    set alignment-work alignment-work + actual-social-work

    set heading heading + ((1 - effective-inertia) * desired-turn)
  ]
end

to apply-noise
  set heading heading + (noise-level * (random-float 2 - 1) * 30)
end

to move
  fd 0.3
end

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; PERTURBATION
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

to perturb
  ;; Only start new perturbation if not already perturbing
  if not perturbing? [
    set perturbing? true
    set perturb-timer perturb-duration
    set perturb-started? true
    set last-perturb-strength perturbation-strength

    ;; Reset recovery tracking for this perturbation
    set peak-deviation 0
    set recovery-complete? false
    ;; NOTE: stable-baseline is NOT reset - it retains the rolling pre-perturbation value
  ]
end

to apply-perturbation
  ask turtles [
    set heading heading + (random-float perturbation-strength - perturbation-strength / 2)
  ]

  set perturb-timer perturb-timer - 1
  if perturb-timer <= 0 [
    set perturbing? false
    set perturb-end-tick ticks  ;; Mark when perturbation ended - recovery timing starts here
  ]
end

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; REPORTERS
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

to-report heading-variance
  if not any? turtles [
    report 0
  ]

  let mean-heading mean [heading] of turtles
  report mean [ abs subtract-headings heading mean-heading ] of turtles
end

;; Additional reporters for BehaviorSpace experiments
to-report recovery-time
  ;; Returns last-recovery-time, or -1 if recovery not yet complete
  report last-recovery-time
end

to-report baseline
  ;; Returns the stable baseline variance (rolling average)
  report stable-baseline
end

to-report max-deviation
  ;; Returns peak deviation from baseline during/after perturbation
  report peak-deviation
end

to-report is-recovering?
  ;; Returns true if system is still recovering from perturbation
  report not recovery-complete?
end

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; COST REPORTERS
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

to-report cumulative-cost
  ;; Total cost for a turtle (sum of all cost types)
  report metabolic-cost + shock-cost + recovery-cost
end

to-report mean-metabolic-cost
  if not any? turtles [ report 0 ]
  report mean [metabolic-cost] of turtles
end

to-report mean-shock-cost
  if not any? turtles [ report 0 ]
  report mean [shock-cost] of turtles
end

to-report mean-recovery-cost
  if not any? turtles [ report 0 ]
  report mean [recovery-cost] of turtles
end

to-report mean-cumulative-cost
  if not any? turtles [ report 0 ]
  report mean [cumulative-cost] of turtles
end

to-report cost-variance
  ;; Variance in cumulative cost across agents (distributional burden)
  if not any? turtles [ report 0 ]
  report variance [cumulative-cost] of turtles
end

to-report max-agent-cost
  ;; Highest individual burden
  if not any? turtles [ report 0 ]
  report max [cumulative-cost] of turtles
end

to-report min-agent-cost
  ;; Lowest individual burden
  if not any? turtles [ report 0 ]
  report min [cumulative-cost] of turtles
end

to-report cost-range-ratio
  ;; Ratio of max to min cost (simpler inequality measure)
  if not any? turtles [ report 0 ]
  let min-cost min [cumulative-cost] of turtles
  if min-cost <= 0 [ report 0 ]
  report (max [cumulative-cost] of turtles) / min-cost
end

to-report cost-gini
  ;; Gini coefficient of cumulative cost distribution (0 = equal, 1 = concentrated)
  if not any? turtles [ report 0 ]
  let costs sort [cumulative-cost] of turtles
  let n length costs
  if n < 2 [ report 0 ]

  let total sum costs
  if total <= 0 [ report 0 ]

  ;; Compute Gini using sorted values
  let weighted-sum 0
  let i 1
  foreach costs [ c ->
    set weighted-sum weighted-sum + (i * c)
    set i i + 1
  ]

  ;; Gini formula: G = (2 * sum(i * x_i)) / (n * sum(x_i)) - (n + 1) / n
  report (2 * weighted-sum) / (n * total) - (n + 1) / n
end

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; FATIGUE REPORTERS (v1)
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

to-report calculate-fatigue-level
  ;; Per-agent fatigue level (0 = no fatigue, 1 = max fatigue)
  ;; Based on cumulative cost relative to threshold and saturation
  let my-cumulative metabolic-cost + shock-cost + recovery-cost
  ifelse my-cumulative <= fatigue-threshold [
    report 0
  ] [
    report min list 1 ((my-cumulative - fatigue-threshold) / (fatigue-saturation - fatigue-threshold))
  ]
end

to-report mean-fatigue-level
  ;; Average fatigue across all agents
  if not any? turtles [ report 0 ]
  report mean [calculate-fatigue-level] of turtles
end

to-report max-fatigue-level
  ;; Highest fatigue in the population
  if not any? turtles [ report 0 ]
  report max [calculate-fatigue-level] of turtles
end

to-report agents-fatigued
  ;; Count of agents above fatigue threshold
  if not any? turtles [ report 0 ]
  report count turtles with [cumulative-cost > fatigue-threshold]
end

to-report mean-effective-inertia
  ;; Average effective inertia (includes fatigue effect when enabled)
  if not any? turtles [ report 0 ]
  ifelse fatigue-enabled? [
    report mean [inertia + ((1 - inertia) * fatigue-intensity * calculate-fatigue-level)] of turtles
  ] [
    report mean [inertia] of turtles
  ]
end

to-report fatigue-gini
  ;; Gini coefficient of fatigue distribution
  if not any? turtles [ report 0 ]
  let fatigue-levels sort [calculate-fatigue-level] of turtles
  let n length fatigue-levels
  if n < 2 [ report 0 ]

  let total sum fatigue-levels
  if total <= 0 [ report 0 ]

  let weighted-sum 0
  let i 1
  foreach fatigue-levels [ f ->
    set weighted-sum weighted-sum + (i * f)
    set i i + 1
  ]

  report (2 * weighted-sum) / (n * total) - (n + 1) / n
end

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;; I004: CONFIGURATION TRACKING REPORTERS
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;

to-report mean-heading-of-turtles
  ;; Circular mean heading using vector average
  if not any? turtles [ report 0 ]
  let mean-x mean [sin heading] of turtles
  let mean-y mean [cos heading] of turtles
  ifelse mean-x = 0 and mean-y = 0 [
    report 0
  ] [
    report atan mean-x mean-y
  ]
end

to-report at-risk-agents
  ;; Agents with high coupling AND far from initial mean
  ;; Thresholds: coupling > 0.7 (top 30%), distance > 45° (far from consensus)
  report turtles with [coupling-bias > 0.7 and initial-distance-from-mean > 45]
end

to-report at-risk-count
  report count at-risk-agents
end

to-report at-risk-fraction
  if not any? turtles [ report 0 ]
  report count at-risk-agents / count turtles
end

to-report at-risk-mean-cost
  let at-risk at-risk-agents
  if not any? at-risk [ report 0 ]
  report mean [cumulative-cost] of at-risk
end

to-report at-risk-mean-alignment-work
  let at-risk at-risk-agents
  if not any? at-risk [ report 0 ]
  report mean [alignment-work] of at-risk
end

to-report at-risk-max-fatigue
  let at-risk at-risk-agents
  if not any? at-risk [ report 0 ]
  report max [calculate-fatigue-level] of at-risk
end

to-report at-risk-mean-fatigue
  let at-risk at-risk-agents
  if not any? at-risk [ report 0 ]
  report mean [calculate-fatigue-level] of at-risk
end

to-report rest-agents
  ;; All agents NOT at-risk
  report turtles with [not (coupling-bias > 0.7 and initial-distance-from-mean > 45)]
end

to-report rest-mean-cost
  let rest rest-agents
  if not any? rest [ report 0 ]
  report mean [cumulative-cost] of rest
end

to-report rest-mean-alignment-work
  let rest rest-agents
  if not any? rest [ report 0 ]
  report mean [alignment-work] of rest
end

to-report rest-mean-fatigue
  let rest rest-agents
  if not any? rest [ report 0 ]
  report mean [calculate-fatigue-level] of rest
end

to-report cost-burden-ratio
  ;; Ratio of at-risk cost to rest-of-population cost
  ;; >1 means at-risk agents bear disproportionate burden
  let rest-cost rest-mean-cost
  if rest-cost <= 0 [ report 0 ]
  report at-risk-mean-cost / rest-cost
end

to-report alignment-work-ratio
  ;; Ratio of at-risk alignment work to rest
  let rest-work rest-mean-alignment-work
  if rest-work <= 0 [ report 0 ]
  report at-risk-mean-alignment-work / rest-work
end

to-report fatigue-burden-ratio
  ;; Ratio of at-risk fatigue to rest
  let rest-fatigue rest-mean-fatigue
  if rest-fatigue <= 0 [ report 0 ]
  report at-risk-mean-fatigue / rest-fatigue
end

to-report mean-initial-distance
  ;; Average initial distance from mean (for summary)
  if not any? turtles [ report 0 ]
  report mean [initial-distance-from-mean] of turtles
end

to-report mean-alignment-work
  ;; Average alignment work across all agents
  if not any? turtles [ report 0 ]
  report mean [alignment-work] of turtles
end

to-report max-alignment-work
  ;; Highest alignment work (who worked hardest)
  if not any? turtles [ report 0 ]
  report max [alignment-work] of turtles
end

to-report alignment-work-gini
  ;; Gini coefficient of alignment work distribution
  if not any? turtles [ report 0 ]
  let work-values sort [alignment-work] of turtles
  let n length work-values
  if n < 2 [ report 0 ]

  let total sum work-values
  if total <= 0 [ report 0 ]

  let weighted-sum 0
  let i 1
  foreach work-values [ w ->
    set weighted-sum weighted-sum + (i * w)
    set i i + 1
  ]

  report (2 * weighted-sum) / (n * total) - (n + 1) / n
end
]]></code>
